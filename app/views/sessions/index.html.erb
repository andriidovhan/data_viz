<h1>Data visualization</h1>


  <div class="col-xs-6">
    <h3>Amount of build per day</h3>
    <%= line_chart @sessions.group_by_day(:created_at, format: "%B %d, %Y").count %>

    <h3>Amount of build per day(granulated)</h3>
    <%= line_chart @sessions.group(:summary_status).group_by_day(:created_at).count %>

    <h3>Build duration</h3>
    <%= line_chart @sessions.pluck(:created_at, :duration), xtitle: "Day", ytitle: "Duration" %>
  </div>
